package TypeCasting;
import java.util.*;

public class Numbergame {

	public static void main(String[] args) {
		int i = 0;
		
		for(i=0; i<=100; i++) {
			if(i%3 != 0 & i%5 !=0)
			{
			System.out.println(i);
			}else
			if(i%3 == 0 & i%5 !=0)
			{
				System.out.println("fizz");
			}
			else if(i%3 ==0 & i%5==0)
			{
				System.out.println("fizz buzz");
			}
			
		}

	}

}
