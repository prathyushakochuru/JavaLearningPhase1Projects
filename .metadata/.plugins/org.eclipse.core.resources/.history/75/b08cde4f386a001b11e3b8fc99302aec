package CollectionsOp;
import java.util.*;
import java.util.Iterator;

public class Dequeue {

	public static void main(String[] args) {
		 Deque<String> dq= new LinkedList<String>();
	      dq.add("a.txt");
	      dq.addFirst("b.txt");
	      dq.addLast("c.txt");
	      dq.push("d.txt");
	      dq.offer("e.txt");
	      dq.offerFirst("f.txt");
	      dq.offerLast("a.txt");
	      
	      //reading dequeue 
	      System.out.println("The deque is: " + dq);
	      System.out.print("\nDequeue using standard Iterator: ");
	      Iterator i = dq.iterator();
	      while (i.hasNext()) {
	         System.out.print(i.next() + " ");
	      }
	      System.out.println("\n\nUsing peek, the element at head of the deque is: " + dq.peek());
	      System.out.println("The deque after peek: " + dq);
	      System.out.println("\nUsing pop, the element removed from the head of the deque is: " + dq.pop());
	      System.out.println("The deque after pop: " + dq);
	      System.out.println("\nDoes the deque contain element 8: " + dq.contains("8"));
	      dq.removeFirst();
	      dq.removeLast();
	      System.out.println("\nDeque after removing the first and last elements is: " + dq);

	}

}
